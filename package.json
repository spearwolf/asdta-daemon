{
  "name": "asdta-daemon",
  "version": "0.0.1",
  "description": "the allow self-determined time allocation daemon",
  "bin": {
    "asdta": "./asdta-cli.mjs"
  },
  "scripts": {
    "prettier:check": "prettier --check \"./**/*.{js,mjs}\"",
    "prettier:write": "prettier --write \"./**/*.{js,mjs}\"",
    "eslint": "eslint \"./*\" --ext .js,.mjs",
    "eslint:fix": "eslint \"./*\" --ext .js,.mjs --fix",
    "lint": "run-s eslint prettier:check",
    "fix": "run-s eslint:fix prettier:write"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime-corejs3": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-problems": "^5.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "node-notifier": "^8.0.0",
    "systeminformation": "^4.27.8"
